{
  "name": "@newrelic/nr-querypack",
  "version": "3.12.0",
  "description": "querypack is a serialization format used by the browser agent",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "generate-parser": "pegjs ./lib/parser/querypack.pegjs ./lib/parser/querypack.js",
    "pretest": "npm run build && npm run lint",
    "test": "node ./test/index.js",
    "build": "node ./build.js",
    "precommit": "npm run lint && npm run build",
    "prepublish": "npm run build",
    "set-version": "npm version --no-git-tag-version $ACTUAL_VERSION && npm run commit-version",
    "commit-version": "git -c user.name='jenkins-build-bot' -c user.email='<jenkins_build_bot@newrelic.com>' commit -am \"bump npm version to v$ACTUAL_VERSION\"",
    "clear-generated": "rm -rf src/main/java/com/newrelic/querypack/bel3"
  },
  "contributors": [
    "Michael Hayes <mhayes@newrelic.com>",
    "Katie Leonard <kleonard@newrelic.com>",
    "Ben Weintraub <ben@newrelic.com>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "concat-stream": "^1.5.0",
    "joi": "^7.1.0",
    "yargs": "^3.27.0"
  },
  "bin": {
    "qputil": "./bin/qputil.js"
  },
  "devDependencies": {
    "glob": "^5.0.15",
    "tape": "^4.2.2",
    "pegjs": "^0.9.0",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "husky": "^0.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newrelic/nr-querypack.git"
  }
}
